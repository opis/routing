{
    "name": "opis/routing",
    "description": "A framework for building routing components",
    "keywords": ["opis", "routing", "serializable", "framework", "http", "events"],
    "homepage": "http://opis.io",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Marius Sarca",
            "email": "marius.sarca@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "opis/closure": "^2.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.4.*"
    },
    "autoload": {
        "psr-4": {
            "Opis\\Routing\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Opis\\Routing\\Test\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.0.x-dev"
        }
    }
}
