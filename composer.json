{
    "name": "opis/routing",
    "description": "A framework for building packages that need routing capabilities",
    "keywords": ["opis", "routing", "serializable", "framework"],
    "homepage": "http://opis.io",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Marius Sarca",
            "email": "marius.sarca@gmail.com"
        },
        {
            "name": "Sorin Sarca",
            "email": "sarca_sorin@hotmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "opis/closure": "^3.0",
        "opis/pattern": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "Opis\\Routing\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Opis\\Routing\\Test\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "5.0.x-dev"
        }
    }
}
